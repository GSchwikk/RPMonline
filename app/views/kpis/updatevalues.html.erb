<p id="notice"><%= notice %></p>

<h1>Listing all Key Performance Indicators</h1>

<table class="table table-hover table-condensed">
  <thead>
    <tr>
      <th width ="10%">Name</th>
      <th width ="10%">Units</th>
      <th colspan="4"></th>
    </tr>
  </thead>

  <tbody>
    <% @kpis.each do |kpi| %>
      <tr>
        <td width ="10%"><%= kpi.name %></td>
        <td width ="10%"><%= kpi.units %></td>
        
        <td></td>
        <td></td>
        <td></td>
        <td></td>
      </tr>
    <% end %>
  </tbody>
</table>